<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.8"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.8 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.8.xsd"
                    name="CSharp">
    <description>Provides a scanner for CSharp Source code.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.UsingDirectiveDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.ClassDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.DelegateDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.EnumDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.FieldDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.InterfaceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.MethodDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.NamespaceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.ParameterDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.StructDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.TypedDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.TypedDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.ValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.VariableDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.csharp.scanner.CSharpAsXMLScannerPlugin</class>
    </scanner>
</jqassistant-plugin>