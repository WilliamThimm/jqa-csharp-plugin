<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.8"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.8 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.8.xsd"
                    name="CSharp">
    <description>Provides a scanner for CSharp Source code.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.csharp.scanner.CSharpScannerPlugin</class>
    </scanner>
</jqassistant-plugin>